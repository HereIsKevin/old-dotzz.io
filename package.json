{
  "name": "dotzz.io",
  "type": "module",
  "version": "0.1.0",
  "description": "Simple but addictive MMO game",
  "private": true,
  "scripts": {
    "format:check": "prettier --check ./*.json ./packages/*/*.{html,css,js,json} ./packages/*/src/*.ts",
    "format:write": "prettier --write ./*.json ./packages/*/*.{html,css,js,json} ./packages/*/src/*.ts",
    "lint:check": "eslint ./packages/*/src/*.ts",
    "lint:fix": "eslint --fix ./packages/*/src/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HereIsKevin/dotzz.io.git"
  },
  "author": "Kevin Feng",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/HereIsKevin/dotzz.io/issues"
  },
  "homepage": "https://github.com/HereIsKevin/dotzz.io#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "workspaces": [
    "packages/*"
  ]
}
