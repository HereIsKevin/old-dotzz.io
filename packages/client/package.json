{
  "name": "client",
  "version": "0.1.0",
  "description": "Client for dotzz.io",
  "scripts": {
    "build:tsc": "tsc",
    "build:rollup": "rollup --config",
    "build:terser": "terser --config-file ./terser.json --output ./dist/index.min.js -- ./dist/index.js",
    "build:development": "npm-run-all --sequential build:tsc build:rollup",
    "build:release": "npm-run-all --sequential build:tsc build:rollup build:terser",
    "watch:tsc": "tsc --watch",
    "watch:rollup": "rollup --watch --config",
    "watch": "npm-run-all --parallel watch:tsc watch:rollup",
    "clean:build": "shx rm -rf ./build/",
    "clean:dist": "shx rm -rf ./dist/",
    "clean": "npm-run-all --parallel clean:build clean:dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HereIsKevin/dotzz.io.git"
  },
  "author": "Kevin Feng",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/HereIsKevin/dotzz.io/issues"
  },
  "homepage": "https://github.com/HereIsKevin/dotzz.io#readme",
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.23.0",
    "shx": "^0.3.2",
    "terser": "^5.0.0",
    "typescript": "^3.9.7"
  }
}
